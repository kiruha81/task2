<h1>Editing Book</h1>
<%= form_with model: @booklist, url: book_path(@booklist.id), method: :patch do |f| %>
  <label>Title</label>
  <%= f.text_field :title %>

  <label>Body</label>
  <%= f.text_area :body %>

  <%= f.submit 'Update Book' %>

  <%= link_to 'Show', book_path(params[:id]) %>
  <%= link_to "Back", books_path(params[:id]) %>
<% end %>